<%= form_with model: product do |form| %>
      <div>
        <%= form.label :name %>
        <%= form.text_field :name %>
      </div>
      <div>
        <%= form.label :des %>
        <%= form.text_field :des %>
      </div>
      <div>
        <%= form.label :price %>
        <%= form.text_field :price %>
      </div>
    <div>
      <%= form.submit %>
    </div>
<% end %>
  